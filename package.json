{
  "name": "sole-bot",
  "version": "0.2.0",
  "description": "The most versatile sneaker bot.",
  "main": "./dist/main/main.js",
  "scripts": {
    "build-backend": "babel src/main --out-dir dist/main --copy-files --extensions \".js,.ts\" --no-copy-ignored --ignore \"src/renderer/**/*\"",
    "build-main": "yarn build-backend&&javascript-obfuscator ./dist/main --output .",
    "build-renderer": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.renderer.prod.config.js",
    "build": "yarn build-main && yarn build-renderer",
    "start-renderer-dev": "webpack-dev-server --config ./webpack/webpack.renderer.dev.config.js",
    "start-main-dev": "yarn build-backend&&cross-env NODE_ENV=development electron .",
    "dev": "cross-env START_HOT=1 yarn start-renderer-dev",
    "dist": "yarn build&&cross-env NODE_ENV=production electron-builder",
    "deploy": "yarn build&&cross-env NODE_ENV=production electron-builder --publish=always",
    "package-win": "yarn build&&cross-env NODE_ENV=production electron-builder --win --x64",
    "postinstall": "electron-builder install-app-deps",
    "format": "prettier --write \"**/*.{ts,js,scss,html}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "build": {
    "productName": "SoleAIO",
    "asarUnpack": "node_modules/puppeteer/.local-chromium/**/*",
    "publish": {
      "provider": "s3",
      "bucket": "gad0h20ehijdf",
      "region": "us-west-1"
    },
    "mac": {
      "category": "public.app-category.business"
    },
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*"
    ],
    "linux": {
      "target": "deb"
    },
    "win": {
      "target": "nsis"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-regenerator": "^7.8.3",
    "@babel/plugin-transform-typescript": "^7.8.7",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.8.3",
    "@hot-loader/react-dom": "^16.11.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.0",
    "electron": "8.3.0",
    "electron-builder": "22.2.0",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "image-webpack-loader": "^6.0.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "react-test-renderer": "^16.12.0",
    "sass-loader": "^8.0.0",
    "spectron": "^9.0.0",
    "style-loader": "^1.1.2",
    "typescript": "^3.8.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2",
    "webpack-obfuscator": "^0.20.0"
  },
  "dependencies": {
    "@types/cheerio": "^0.22.16",
    "@types/form-data": "^2.5.0",
    "@types/got": "^9.6.11",
    "@types/lodash": "^4.14.150",
    "@types/request-promise-native": "^1.0.17",
    "@types/uuid": "^7.0.2",
    "autosolve-client": "^2.0.0",
    "axios": "^0.19.2",
    "card-validator": "^6.2.0",
    "cheerio": "^1.0.0-rc.3",
    "cloudscraper": "^4.6.0",
    "cookie-parse": "^0.4.0",
    "current-processes": "^0.2.1",
    "discord-rich-presence": "^0.0.8",
    "electron-updater": "^4.3.1",
    "express": "^4.17.1",
    "form-data": "^3.0.0",
    "got": "^11.5.1",
    "hpagent": "^0.1.1",
    "jquery": "^3.4.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-adyen-encrypt": "^2.2.0",
    "node-machine-id": "^1.1.12",
    "pretty-checkbox": "^3.0.3",
    "pretty-checkbox-react": "^1.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-country-region-selector": "^1.4.7",
    "react-countup": "^4.3.3",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.19",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scroll-text": "^1.0.1",
    "react-select": "^3.0.8",
    "react-spring": "^8.0.27",
    "react-tilt": "^0.1.4",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8",
    "toasted-notes": "^3.2.0",
    "tough-cookie": "^4.0.0",
    "underscore": "^1.9.2",
    "uuid": "^7.0.1",
    "uuidv4": "^6.2.4",
    "webhook-discord": "^3.5.1"
  }
}
